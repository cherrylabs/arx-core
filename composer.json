    "keywords": ['arx'],
    "homepage": "http://arx.xxx",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "classpreloader/classpreloader": "1.0.*",
        "doctrine/dbal": "2.4.x",
        "ircmaxell/password-compat": "1.0.*",
        "filp/whoops": "1.0.6",
        "monolog/monolog": "1.5.*",
        "nesbot/carbon": "1.*",
        "patchwork/utf8": "*",
        "predis/predis": "0.8.*",
        "swiftmailer/swiftmailer": "5.0.*",
        "symfony/browser-kit": "2.3.*",
        "symfony/console": "2.3.*",
        "symfony/css-selector": "2.3.*",
        "symfony/debug": "2.3.*",
        "symfony/dom-crawler": "2.3.*",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/finder": "2.3.*",
        "symfony/http-foundation": "2.3.*",
