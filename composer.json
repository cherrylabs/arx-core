{
    "name": "arx/core",
    "description": "Arx as a library",
    "homepage": "http://arx.xxx",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "laravel/framework" : "*",
        "raveren/kint" : "*",
        "j4mie/idiorm" : "*",
        "mustache/mustache" : "*",
        "slim/slim" : "*",
        "slim/extras" : "*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Arx": "/modules/arx/core/src/"
        },
        "autoload": {
            "classmap": ["/modules/arx/vendor"]
        },
        "files" : ["/modules/arx/core/src/core.php"]
    },

    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],

    "minimum-stability": "dev"
}
